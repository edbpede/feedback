# Copy this file to .env and fill in your values

# SHA-256 hash of the access password
# Generate with: echo -n "yourPassword" | sha256sum
PASSWORD_HASH=e3b0c44298fc1c149afbf4c8996fb92427ae41e4649b934ca495991b7852b855

# Random string for signing session cookies (32+ chars)
# Generate with: openssl rand -base64 32
SESSION_SECRET=your-session-secret-here-minimum-32-characters

# API key from nano-gpt.com
NANO_GPT_API_KEY=ng_your_api_key_here

# Optional: Model identifier (defaults to TEE/deepseek-v3.2)
NANO_GPT_MODEL=TEE/deepseek-v3.2

# Optional: API base URL (defaults to https://nano-gpt.com/api/v1)
API_BASE_URL=https://nano-gpt.com/api/v1

# Optional: SHA-256 hash for enhanced quality password
# When set, users must enter this password to access "Forbedret kvalitet" option
# When not set, the enhanced quality option is freely available (no password required)
# Generate with: echo -n "yourPassword" | sha256sum
ENHANCED_QUALITY_PASSWORD_HASH=
